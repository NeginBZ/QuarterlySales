@model TopRatedEmployeeViewModel
@{
    @using QuarterlySales.Components
}

<h3>Top Quarters By Employee</h3>
<h6>
    (i.e. Rated at least @Model.LowestRating)
</h6>
<ul>
    @for (int i = 0; i < Model.numberOfUsersToDisplay; i++)
    {
    <li>@Model.users[@i].Year (@Model.users[@i].Employee.Fullname @Model.users[@i].Amount )</li>
    }
</ul>

